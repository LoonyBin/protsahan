# This is a checklist of things to do to install the encouragement store.
# Install git
# install rvm
  bash < <(curl -s https://rvm.beginrescueend.com/install/rvm)
# install ruby
  rvm install 1.9.2
# create gemset
  rvm gemset create protsahan
# clone protsahan
  git clone git://github.com/LoonyBin/protsahan.git
  cd protsahan

# install image magick
# install gems
  gem install bundler
  bundle install
# configure database
# on Development machine
  cp config/database.yaml{.example,}
